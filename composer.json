{
    "name": "phly/phly-comic",
    "description": "Library for fetching various web comic sources",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "comics",
        "console"
    ],
    "homepage": "https://github.com/weierophinney/PhlyComic",
    "authors": [
        {
            "name": "Matthew Weier O'Phinney",
            "email": "matthew@weierophinney.net",
            "homepage": "http://mwop.net/"
        }
    ],
    "support": {
        "source": "https://github.com/weierophinney/PhlyComic",
        "issues": "https://github.com/weierophinney/PhlyComic/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
        "psr/container": "^1.1 || ^2.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "symfony/console": "^5.0 || ^6.0",
        "spatie/async": "^1.5",
        "symfony/css-selector": "^7.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "php-http/curl-client": "^2.3.2",
        "nyholm/psr7": "^1.8",
        "laminas/laminas-coding-standard": "~2.3.0"
    },
    "autoload": {
        "psr-4": {
            "PhlyComic\\": "src/"
        }
    },
    "bin": ["bin/phly-comic.php"],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
